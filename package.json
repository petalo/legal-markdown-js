{
  "name": "legal-md-wizard",
  "version": "0.1.0",
  "description": "Node.js implementation of LegalMarkdown for processing legal documents with markdown and YAML",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "cli": "ts-node src/cli.ts"
  },
  "keywords": [
    "legal",
    "markdown",
    "document",
    "contract",
    "yaml",
    "parser",
    "legal-markdown"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "marked": "^9.1.4",
    "chalk": "^5.3.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.8",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "bin": {
    "legal-md": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}